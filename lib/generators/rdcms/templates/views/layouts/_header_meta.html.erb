<!-- basic_headers -->
<%= basic_headers() %>
<!-- Style files -->
<%= stylesheet_link_tag     "application", :media => "all" %>
<%= stylesheet_link_tag     "application", :media => "print" %>
<%= yield :style %>

<!-- Script files -->
<%= javascript_include_tag "application" %>
<%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=true", "gmaps" %>

<script src="/gg.analytics.js"> </script>
<%= yield :script %>

<%= csrf_meta_tags %>

<% if params[:pdf].present? && params[:pdf] == "1" %>
  <%#= stylesheet_link_tag "pdf", :media => "print" %>
<% else %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
<% end %>
